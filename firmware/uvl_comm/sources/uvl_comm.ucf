NET "COM_ADC_D<2>"   LOC = E18 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE ; 
NET "COM_ADC_D<3>"   LOC = F17 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE ; 
