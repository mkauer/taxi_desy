NET "COM_ADC_D<5>"   LOC = H18 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE ; 
NET "COM_ADC_D<6>"   LOC = H17 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE ; 
NET "COM_ADC_D<7>"   LOC = J18 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE ; 
NET "COM_ADC_D<8>"   LOC = J16 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE ; 
NET "COM_ADC_D<9>"   LOC = K18 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE ; 
NET "COM_ADC_D<10>"  LOC = K17 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE ; 
