NET "COM_ADC_SCLK"  LOC = C17 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE | DRIVE = 2 | SLEW = SLOW ; 
NET "COM_ADC_CSBn"  LOC = C18 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE | DRIVE = 2 | SLEW = SLOW ; 
NET "COM_ADC_SDIO"  LOC = D18 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE | DRIVE = 2 | SLEW = SLOW ; 

