# pin assignment compatible both,  for XC6SLX16-3CSG324I and XC6SLX45-3CSG324I
# prevent config. signals from being used
CONFIG PROHIBIT = R13; # DIN
CONFIG PROHIBIT = D4 ; # HSWAPEN
CONFIG PROHIBIT = T15; # M0
CONFIG PROHIBIT = N12; # M1
CONFIG PROHIBIT = T13; # MOSI
CONFIG PROHIBIT = V3 ; # CSO_B
CONFIG PROHIBIT = R15; # CCLK
CONFIG PROHIBIT = U3 ; # INIT_B
#CONFIG PROHIBIT = T20; "DOUT_BUSY"
#
# for migration to XC6SLX45
# DO NOT USE  C12; # 
# DO NOT USE  D12; # 
# DO NOT USE  E6 ; # 
# DO NOT USE  E7 ; # 
# DO NOT USE  E8 ; # 
# DO NOT USE  E11; # 
# DO NOT USE  E12; # 
# DO NOT USE  F7 ; # 
# DO NOT USE  F8 ; # 
# DO NOT USE  F10; # 
# DO NOT USE  F11; # 
# DO NOT USE  F12; # 
# DO NOT USE  G8 ; # 
# DO NOT USE  G11; # 

#NET "QOSC_OUT" TNM_NET = "qosc";
#TIMESPEC "TS_qosc" = PERIOD "qosc" 40 ns HIGH 50%;

NET "clk" TNM_NET = "sys_clk";
TIMESPEC "TS_sys_clk" = PERIOD "sys_clk" 16.7 ns HIGH 50%;
NET "com_clk" TNM_NET = "com_clk";
TIMESPEC "TS_com_clk" = PERIOD "com_clk" 16.7 ns HIGH 50%;


# Bank_1

NET "COM_ADC_D<0>"   LOC = D17 | IOSTANDARD = LVCMOS25 | IOBDELAY = NONE ; 
